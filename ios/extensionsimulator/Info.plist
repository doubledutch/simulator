	<string></string>
	<key>NSAppTransportSecurity</key>
	<!--See http://ste.vn/2015/06/10/configuring-app-transport-security-ios-9-osx-10-11/ -->
	<dict>
    <key>NSAllowsArbitraryLoads</key>
    <true/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>localhost</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>NSCameraUsageDescription</key>
	<string>This will allow you to post to the activity feed and update your profile image.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>This allows us to let you choose which photos to post to the activity feed or use as your profile image.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>This allows us to add the photos you take to your camera roll.</string>
